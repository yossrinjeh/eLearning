{
    "version": 2,
    "builds": [
      {
        "src": "api/index.php",
        "use": "vercel-php@0.6.0"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ],
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "false",
      "APP_URL": "https://e-learning-rho-beryl.vercel.app",
      "APP_KEY": "base64:YpzzuU2bovqR2tn8Xcrhjl6VbYb+e5p6R7bPXFUQd6w",
      "APP_CONFIG_CACHE": "/tmp/config.php",
      "APP_EVENTS_CACHE": "/tmp/events.php",
      "APP_PACKAGES_CACHE": "/tmp/packages.php",
      "APP_ROUTES_CACHE": "/tmp/routes.php",
      "APP_SERVICES_CACHE": "/tmp/services.php",
      "VIEW_COMPILED_PATH": "/tmp",
      "CACHE_DRIVER": "array",
      "LOG_CHANNEL": "stderr",
      "SESSION_DRIVER": "cookie",
      "INSTALL_OPENSSL": "true" 
    },
    "build": {
      "env": {
        "INSTALL_OPENSSL": "true"
      }
    }
 
  }
  